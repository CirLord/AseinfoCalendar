@model IEnumerable<AseinfoCalendar.Models.etp_etapas>

@{
    ViewBag.Title = "Etapas";
}

<h2>Etapas</h2>

<p>
    @Html.ActionLink("Nueva Etapa", "Create")
</p>
<table class="table table-responsive">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.etp_nombre)
        </th>
        <th class="hidden-xs hidden-sm">
            @Html.DisplayNameFor(model => model.etp_orden)
        </th>
        <th class="hidden-xs hidden-sm">
            @Html.DisplayNameFor(model => model.etp_descripciom)
        </th>
        <th class="hidden-xs hidden-sm">
            @Html.DisplayNameFor(model => model.etp_objetivo)
        </th>
        <th class="hidden-xs">
            @Html.DisplayNameFor(model => model.etp_fecha_inicio)
        </th>
        <th class="hidden-xs">
            @Html.DisplayNameFor(model => model.etp_fecha_fin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pln_plan.pln_nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.etp_nombre)
        </td>
        <td class="hidden-xs hidden-sm">
            @Html.DisplayFor(modelItem => item.etp_orden)
        </td>
        <td class="hidden-xs hidden-sm">
            @Html.DisplayFor(modelItem => item.etp_descripciom)
        </td>
        <td class="hidden-xs hidden-sm">
            @Html.DisplayFor(modelItem => item.etp_objetivo)
        </td>
        <td class="hidden-xs">
            @Html.DisplayFor(modelItem => item.etp_fecha_inicio)
        </td>
        <td class="hidden-xs">
            @Html.DisplayFor(modelItem => item.etp_fecha_fin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pln_plan.pln_nombre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.etp_codigo }) |
            @Html.ActionLink("Details", "Details", new { id=item.etp_codigo }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.etp_codigo })
        </td>
    </tr>
}

</table>
